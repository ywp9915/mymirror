page {
  background:#f6f6f6;
  font-size: 30rpx;
  font-family: PingFang-SC-Bold;
}

.page{
  display: flex;
  flex-direction: column;
  margin:0 27rpx;
}

.present-box{
  display: flex;
  justify-content:space-between;
  height:198rpx;
  width:100%;
  border-radius:15rpx;
  margin:29rpx 0; 
  background:#fff;
}
.present-info{
  padding-left:28rpx;
}
.present-info .card-account{
  margin-top:38rpx;
}
.present-info .serial-number{
  margin:18rpx 0;
}
.present-info .info{
  font-size:24rpx;
  color:#a6a6a6;
}
.present-state{
  text-align:center;
  margin-right:21rpx;
  padding-top:50rpx;
  color:#fe6030;
}
.present-state .state{
  width:135rpx;
  height:45rpx;
  margin:18rpx auto 0 auto;
  border-radius:8rpx;
  color:#fff;
}
/* 提现失败 */
.present-state .a{
  background-color: #e63333;
}
/* 未审核 */
.present-state .b{
  background-color: #aaa;
}
/* 提现成功 */
.present-state .c{
  background-color: #3791f4;
}
/* 已审核 */
.present-state .d{
  background-color: #f09135;
}
/* 已提交 */
.present-state .e{
  background-color: #1dcb4a;
}

